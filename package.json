{
  "name": "pty-themes",
  "version": "1.1.6",
  "description": "Collection of style definitions for PTY platforms",
  "main": "index.js",
  "scripts": {
    "storybook:bol": "export STORYBOOK_THEME=bol && start-storybook",
    "storybook:wild": "export STORYBOOK_THEME=wild && start-storybook",
    "build": "sass src/sass/themes:dist --style compressed",
    "lint": "sass-lint -c .sass-lint.yml 'src/sass/**/*.scss' -v -q",
    "prepare": "npm run build",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vbence86/pty-themes.git"
  },
  "author": "Bence Varga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vbence86/pty-themes/issues"
  },
  "homepage": "https://github.com/vbence86/pty-themes#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/html": "^5.3.18",
    "@storybook/preset-scss": "^1.0.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "lodash": "^4.17.15",
    "sass": "^1.26.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1"
  },
  "sasslintConfig": ".sass-lint.yml",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "normalize-scss": "^7.0.1"
  }
}
